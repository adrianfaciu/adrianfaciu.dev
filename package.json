{
  "version": "3.0.0",
  "name": "gatsby-starter-lumen",
  "description": "A minimal, lightweight and mobile-first starter for creating blazing-fast static blogs",
  "main": "n/a",
  "scripts": {
    "develop": "yarn run clean && gatsby develop",
    "build": "yarn run clean && gatsby build",
    "deploy": "yarn run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "flow-typed": "flow-typed install --overwrite",
    "lint:js": "eslint --cache --ext .js,.jsx .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\" \"yarn flow\"",
    "test": "jest --config ./jest/jest-config.js",
    "test:coverage": "jest --coverage --config ./jest/jest-config.js",
    "test:watch": "jest --watch --config ./jest/jest-config.js",
    "release": "standard-version"
  },
  "repository": "https://github.com/alxshelepenok/gatsby-starter-lumen",
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "netlify-cms",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme",
    "starter",
    "disqus"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "classnames": "^2.3.2",
    "codecov": "^3.8.3",
    "gatsby": "^5.12.6",
    "gatsby-link": "^5.12.0",
    "gatsby-plugin-catch-links": "^5.12.0",
    "gatsby-plugin-feed": "^5.12.1",
    "gatsby-plugin-flow": "^4.12.1",
    "gatsby-plugin-manifest": "^5.12.1",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^6.12.1",
    "gatsby-plugin-sharp": "^5.12.1",
    "gatsby-plugin-sitemap": "^6.12.1",
    "gatsby-remark-autolink-headers": "^6.12.1",
    "gatsby-remark-copy-linked-files": "^6.12.0",
    "gatsby-remark-embed-gist": "^1.2.1",
    "gatsby-remark-images": "^7.12.1",
    "gatsby-remark-katex": "^7.12.0",
    "gatsby-remark-prismjs": "^7.12.0",
    "gatsby-remark-relative-images": "^2.0.5",
    "gatsby-remark-responsive-iframe": "^6.12.0",
    "gatsby-remark-smartypants": "^6.12.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-remark": "^6.12.1",
    "gatsby-transformer-sharp": "^5.12.1",
    "invariant": "^2.2.4",
    "katex": "^0.16.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "netlify-cms": "^2.10.192",
    "sass": "^1.69.3",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-flow-strip-types": "^7.22.5",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-react": "7.22.15",
    "autoprefixer": "10.4.16",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.7.0",
    "babel-preset-gatsby": "3.12.0",
    "browserslist": "4.22.1",
    "concurrently": "8.2.1",
    "eslint": "8.51.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-watch": "8.0.0",
    "flow-bin": "^0.218.0",
    "flow-typed": "^3.9.0",
    "gh-pages": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "lost": "9.0.2",
    "postcss-pxtorem": "6.0.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "5.0.5",
    "standard-version": "9.5.0",
    "stylelint": "15.10.3",
    "stylelint-config-recommended-scss": "13.0.0",
    "stylelint-scss": "5.2.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
