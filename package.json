    "invariant": "^2.2.4",
    "katex": "^0.16.23",
    "lodash": "^4.17.21",
    "modern-normalize": "^3.0.1",
    "moment": "^2.30.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
