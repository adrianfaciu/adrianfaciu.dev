    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-watch": "8.0.0",
    "flow-bin": "^0.254.2",
    "flow-typed": "^4.1.0",
    "gh-pages": "6.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "lost": "9.0.2",
    "postcss-pxtorem": "6.0.0",
    "react-test-renderer": "18.3.1",
    "rimraf": "6.0.1",
